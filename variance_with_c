int variance(int  arr[] , int number){
    int i = 0 ;
    int all = 0 ;
    int average ;
    int varinace ;
    for (i ; i <= number ; i++){
        all += arr[i] ;
    }
    average = all /(number +1);
    all = 0 ;
    for (i = 0 ; i <= number ; i++){
        all += pow((arr[i] - average), 2);
    }
    varinace
}
